@font-face 
  font-family: SFProText-Heavy; 
  src: url('../src/assets/font/SFProText-Heavy.otf')
html,body
  margin: 0px;
  padding 0px;
  font-size: 12px;
  scroll-behavior: smooth;
  overflow-scrolling: touch;
  -webkit-overflow-scrolling: touch;
  color: #171B43;
  // font-family: 'SFProText-Heavy',Helvetica, Arial, sans-serif;
  
a 
  text-decoration: none;
a:hover
  text-decoration: underline;

input[type="password"]
input[type="text"]
input[type="number"]
  margin: 0rem;
  border: 0rem;
  border-radius: 0;
  appearance: none;
  padding: 0rem 0rem 0rem 1rem;
  box-shadow: 0rem 0rem 0rem 0rem;
  width: 100%;
  height:3rem;
  background: #FAFAFA;
  border: 1px solid #E6E6E6;
  -moz-appearance: textfield !important;

input::-webkit-outer-spin-button,
input::-webkit-inner-spin-button
  -webkit-appearance: none !important;
  margin: 0; 
input::-webkit-input-placeholder
  color: rgba(36, 41, 86, 0.4);
  
textarea
  margin: 0rem;
  border: 0rem;
  border-radius: 0;
  appearance: none;
  padding: 1rem !important;
  box-shadow: 0rem 0rem 0rem 0rem;
  width: 100%;
  background: #FAFAFA;
  border: 1px solid #E6E6E6;
textarea::-webkit-input-placeholder
  color: rgba(36, 41, 86, 0.4);


select
  -webkit-appearance: none;/*兼容苹果手机*/


#app
  font-size 12px;
  -webkit-font-smoothing antialiased;
  -moz-osx-font-smoothing grayscale;
.nav
  justify-content space-between
  a
    font-weight bold
    font-size 3rem
  a:link 
    text-decoration: none;
  a:hover 
    text-decoration: none;
  a:visited 
    text-decoration: none;
.link
  font-size:1.17rem;
  font-family:PingFangSC-Regular;
  font-weight:400;
  color:rgba(255,45,85,1);
  line-height:1.67rem;

.in-test
  width: 100%;
.distpicker-address-wrapper
  width: 100% !important;
  display: flex;
  justify-content: space-between;
.distpicker-address-wrapper > select
  width: 49% !important;
  background: #FAFAFA !important;
  border: 1px solid #E6E6E6 !important;
.ivu-timeline-item-content
  padding-left 1.5rem !important;
  padding-bottom: 0rem !important;
.ivu-tabs-nav-container
  font-size:1.17rem !important;
  font-family:PingFangSC-Medium;
  font-weight:500;
  line-height:1.67rem !important;
.ivu-progress-inner
  background-color: rgba(224,226,235,1) !important;
.ivu-progress-bg
  background:linear-gradient(90deg,rgba(233,8,255,1) 0%,rgba(255,5,78,1) 100%) !important;
  // animation: backgroundMove 2s infinite !important;
  // transition: background 2s !important;
.ivu-progress-bg:hover
  background:linear-gradient(90deg,rgba(8,189,255,1) 0%,rgba(206,5,255,1) 100%);  
.ivu-divider
  background: #EBECEF !important;
.ivu-layout-header
  height auto !important; 
.ivu-badge-dot
  background: #FF425D !important;
